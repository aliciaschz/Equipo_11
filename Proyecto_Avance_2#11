            "application/vnd.google.colaboratory.intrinsic+json": {
              "type": "dataframe",
              "variable_name": "promedios_juego"
            }
          },
          "metadata": {},
          "execution_count": 70
        }
      ],
      "source": [
        "# Promedio Bayesiano\n",
        "\n",
        "# Convertir recomendacion a calificacion y año de lanzamiento\n",
        "game['calificacion'] =  game['recommended'].apply(lambda x: 100 if x else 0)\n",
        "\n",
        "# Calcular el promedio simple y el número de calificaciones para cada juego\n",
        "promedios_juego = game.groupby(['app_id','app_name'])['calificacion'].agg(['mean', 'count']).reset_index()\n",
        "promedios_juego.columns = ['app_id', 'app_name','promedio_simple', 'num_calificaciones']\n",
        "\n",
        "# Calcular el promedio global de todas las recomendaciones\n",
        "promedio_global = game['calificacion'].mean()\n",
        "\n",
        "# Definir un valor m para el número mínimo de calificaciones\n",
        "m = 10\n",
        "\n",
        "# Calcular el promedio bayesiano para cada juego\n",
        "promedios_juego['promedio_bayesiano'] = (\n",
        "    (promedios_juego['num_calificaciones'] * promedios_juego['promedio_simple'] + m * promedio_global) /\n",
        "    (promedios_juego['num_calificaciones'] + m)\n",
        ")\n",
        "\n",
        "promedios_juego = pd.merge(promedios_juego,generos, left_on='app_id', right_on='appid', how='left')\n",
        "\n",
        "promedios_juego"
      ]
    },
